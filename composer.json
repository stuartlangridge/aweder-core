{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "askedio/laravel5-profanity-filter": "^1.10",
        "bjeavons/zxcvbn-php": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.2",
        "laravel/ui": "^2.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "mad-web/laravel-initializer": "^3.0",
        "nothingworks/blade-svg": "^0.3.4",
        "olssonm/l5-zxcvbn": "^4.2",
        "predis/predis": "^1.1",
        "stripe/stripe-php": "^7.27",
        "doctrine/dbal": "~2.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "brianium/paratest": "^4.0",
        "dczajkowski/auth-tests": "^7.0",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "laravel/tinker": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.5.2",
        "phpro/grumphp": "^0.18.0",
        "phpstan/phpstan": "^0.12.14",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "^6.1",
        "squizlabs/php_codesniffer": "^3.4",
        "vimeo/psalm": "^3.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 0
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "app-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@composer install",
            "@php artisan app:install",
            "@grumphp-init"
        ],
        "app-update": [
            "@composer install",
            "@php artisan app:update"
        ],
        "app-install-production": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@composer install --no-dev --prefer-dist --optimize-autoloader",
            "@php artisan app:install"
        ],
        "app-update-production": [
            "@composer install --no-dev --prefer-dist --optimize-autoloader",
            "@php artisan app:update"
        ],
        "lint": [
            "vendor/bin/parallel-lint ./ --blame --exclude vendor _ide_helper.php .phpstorm.meta.php"
        ],
        "phpunit": "vendor/bin/phpunit",
        "phpcs": [
            "vendor/bin/phpcs ."
        ],
        "test": [
            "php artisan cache:clear",
            "php vendor/bin/phpunit"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "analyse": [
            "@phpcs",
            "@phpstan"
        ],
        "grumphp-init": [
            "@php vendor/bin/grumphp git:init"
        ],
        "grumphp-deinit": [
            "@php vendor/bin/grumphp git:deinit"
        ]
    }
}
